{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "audit:critical": "npm audit --audit-level=critical",
    "audit:high": "npm audit --audit-level=high",
    "security:check": "npm run audit:high && npm run lint && npm run typecheck",
    "security:report": "npm audit --json > security-audit-report.json && echo 'Security audit report saved to security-audit-report.json'",
    "seed": "ts-node src/scripts/seedMockData.ts",
    "seed:via-api": "ts-node src/scripts/seed-via-api.ts",
    "seed:direct": "ts-node src/scripts/seed-products.ts && ts-node src/scripts/seed-contents.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
